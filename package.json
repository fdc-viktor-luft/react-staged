    "test": "react-scripts test --watchAll=false",
    "lint": "tsc && pnpm eslint"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",

  "peerDependencies": {
    "react": ">= 16.8.0"
  },
  "scripts": {
    "dist": "rm -rf dist && rollup -c && mkdir dist/types && cp src/staged/* dist/types/ && mkdir dist/assets && cp src/assets/staged.scss dist/assets/",

    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "react-use": "^17.2.1",
    "rollup": "^2.44.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass": "^1.32.8",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "browserslist": {
